{
  "name": "foundry-vtt-react-lib",
  "version": "1.0.0",
  "description": "React Lib",
  "private": true,
  "scripts": {
    "build:copy": "copyfiles static/module.json dist",
    "build": "yarn run build:copy && esbuild module=src/module.ts --bundle --outdir=dist",
    "build:production": "yarn run build:copy && esbuild src/module.ts --bundle --minify --outdir=dist",
    "start": "yarn run build:copy && esbuild src/module.ts --bundle --outdir=dist --watch"
  },
  "repository": {},
  "author": "n1xx1",
  "license": "MIT",
  "devDependencies": {
    "@league-of-foundry-developers/foundry-vtt-types": "^0.7.9-6",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.12.25",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
